<h1>Listing inquiries</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Organization</th>
    <th>Email</th>
    <th>Project type</th>
    <th>Budget</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @inquiries.each do |inquiry| %>
  <tr>
    <td><%= inquiry.name %></td>
    <td><%= inquiry.organization %></td>
    <td><%= inquiry.email %></td>
    <td><%= inquiry.project_type %></td>
    <td><%= inquiry.budget %></td>
    <td><%= inquiry.description %></td>
    <td><%= link_to 'Show', inquiry %></td>
    <td><%= link_to 'Edit', edit_inquiry_path(inquiry) %></td>
    <td><%= link_to 'Destroy', inquiry, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Inquiry', new_inquiry_path %>
